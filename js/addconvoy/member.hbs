{{#if this}}
    {{#each this}}
        <li>
            <a href="#" class="item-link item-content"
               id="chooseMember"
               index="{{ @index }}">
                <div class="item-media">
                    <img src="{{ image }}" width="35"/>
                </div>
                <div class="item-inner">
                    <div class="item-title">
                        {{ name }}
                    </div>
                    <div class="item-after">
                        <span class="badge color-blue">
                            abiri
                        </span>
                    </div>
                </div>
            </a>
        </li>
    {{/each}}
{{/if}}